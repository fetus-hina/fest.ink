{
    "name": "jp3cki/inkfest",
    "description": "https://fest.ink/",
    "homepage": "https://fest.ink/",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 7.1",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-pcre": "*",
        "bower-asset/flot": "^0.8.3",
        "bower-asset/fontawesome": "^4.4",
        "bower-asset/jquery": "^3.4.1",
        "bower-asset/timezone-js": "^0.4.13",
        "laminas/laminas-crypt": "^3.3.1",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-smarty": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "yiisoft/yii2-gii": "*"
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower",
            "npm-asset-library": "vendor/npm"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.stat.ink"
        }
    }
}
