{
    "name": "jp3cki/inkfest",
    "description": "https://fest.ink/",
    "homepage": "https://fest.ink/",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.2",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "laminas/laminas-crypt": "^3.11.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-smarty": "*"
    },
    "require-dev": {
        "deployer/deployer": "^7.3.3",
        "squizlabs/php_codesniffer": "^3.8",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "replace": {
        "bower-asset/bootstrap": "*",
        "bower-asset/flot": "*",
        "bower-asset/fontawesome": "*",
        "bower-asset/inputmask": "*",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "*",
        "bower-asset/timezone-js": "*",
        "bower-asset/uglify-js": "*",
        "bower-asset/yii2-pjax": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "platform": {
            "php": "8.2.14"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower",
            "npm-asset-library": "vendor/npm"
        }
    }
}
